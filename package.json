    "eslint": "9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0",
    "prettier": "3.5.2",
    "source-map-support": "0.5.21",
    "supabase": "2.22.6",
    "supertest": "^7.1.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1",
    "vitest": "^3.1.3",
